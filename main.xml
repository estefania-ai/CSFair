<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="#D3D3D3"
    >

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"


        android:layout_marginTop="38dp">

    </FrameLayout>

    <ImageView
        android:layout_width="320dp"
        android:layout_height="180dp"

        android:id="@+id/imageView6"

        android:layout_centerHorizontal="true"
        android:layout_marginTop="21dp" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:weightSum="1"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="31dp"
        android:id="@+id/linearLayout2">

    </LinearLayout>
    <RadioGroup xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    </RadioGroup>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:weightSum="1"
        android:layout_below="@+id/imageView6"

        android:layout_marginEnd="19dp"
        android:layout_marginTop="169dp">

    </LinearLayout>

    <ImageView
        android:layout_width="200dp"
        android:layout_height="180dp"
        app:srcCompat="@drawable/transchi"

        android:layout_centerHorizontal="true"
        android:layout_marginTop="47dp"
        android:id="@+id/imageView8" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/route"
        android:id="@+id/imageButton17"
        android:scaleType="fitCenter"
        android:layout_above="@+id/imageButton16"
        android:layout_alignStart="@+id/imageView8" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/more"
        android:id="@+id/imageButton15"
        android:scaleType="fitCenter"
        android:layout_alignTop="@+id/imageButton17"
        android:layout_toEndOf="@+id/imageButton17" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/mess"
        android:id="@+id/imageButton14"
        android:scaleType="fitCenter"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/about"
        android:id="@+id/imageButton12"
        android:scaleType="fitCenter"
        android:elevation="0dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/imageButton14" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/contact"
        android:id="@+id/imageButton16"
        android:scaleType="fitCenter"
        android:layout_weight="1"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/imageButton14" />

</RelativeLayout>
